#include <stdio.h>

int main() {
    float length, width, area;

    // Input
    printf("Enter the length of the rectangle: ");
    scanf("%f", &length);

    printf("Enter the width of the rectangle: ");
    scanf("%f", &width);

    // Calculating area
    area = length * width;

    // Output
    printf("\nArea of the rectangle = %f\n", area);

    return 0;
}
